<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.4//EN" "https://hibernate.org/dtd/hibernate-mapping-5.4.dtd">
<hibernate-mapping package="com.project">
    <class name="Ciutada" table="ciutada">
        <id name="ciutadaId" column="ciutada_id">
            <generator class="native" />
        </id>
        <property name="nom" column="nom" length="100" not-null="true" />
        <property name="cognom" column="cognom" length="100" not-null="true" />
        <property name="edat" column="edat" not-null="true" />
        <property name="ciutatId" column="ciutat_id" />
    </class>
</hibernate-mapping>
